{
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "generators": {
    "@nrwl/angular:component": {
      "style": "scss"
    }
  },
  "prefix": "app",
  "projectType": "library",
  "sourceRoot": "libs/client-unit-testing/src",
  "tags": ["scope:client-unit-testing", "type:unit-testing"],
  "targets": {
    "lint": {
      "executor": "@angular-eslint/builder:lint",
      "options": {
        "eslintConfig": "libs/client-unit-testing/.eslintrc.json",
        "lintFilePatterns": ["libs/client-unit-testing/**/*.ts"]
      },
      "outputs": ["{options.outputFile}"]
    },
    "prettier-check": {
      "executor": "./tools/executors/prettier:check",
      "options": {
        "config": ""
      },
      "outputs": ["{workspaceRoot}/dist/prettier/libs/client-unit-testing"]
    },
    "stylelint-check": {
      "executor": "./tools/executors/stylelint:check",
      "options": {
        "config": ""
      },
      "outputs": ["{workspaceRoot}/dist/stylelint/libs/client-unit-testing"]
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "options": {
        "jestConfig": "libs/client-unit-testing/jest.config.ts",
        "passWithNoTests": true
      },
      "outputs": ["{workspaceRoot}/coverage/libs/client-unit-testing"]
    },
    "tsc-check": {
      "executor": "./tools/executors/tsc:check",
      "options": {
        "tsConfig": "libs/client-unit-testing/tsconfig.lib.json"
      },
      "outputs": ["dist/out-tsc/libs/client-unit-testing"]
    }
  }
}
