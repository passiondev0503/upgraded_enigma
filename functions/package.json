{
  "name": "upgraded-enigma-server",
  "private": true,
  "description": "Cloud Functions for Firebase",
  "main": "main.js",
  "scripts": {
    "deploy": "firebase deploy --only functions",
    "dist:clean": "rm -f ./main.js ./main.js.map ./*.txt",
    "dist:get": "npm run dist:clear; cp -r ../dist/apps/api/* ./",
    "logs": "firebase functions:log",
    "serve": "firebase serve --only functions",
    "shell": "firebase experimental:functions:shell",
    "start": "npm run shell",
    "start:emulators": "firebase emulators:start --only functions"
  },
  "dependencies": {
    "@nestjs/axios": "0.0.8",
    "@nestjs/common": "8.4.6",
    "@nestjs/core": "8.4.6",
    "@nestjs/jwt": "8.0.1",
    "@nestjs/microservices": "8.4.6",
    "@nestjs/passport": "8.2.1",
    "@nestjs/platform-express": "8.4.6",
    "@nestjs/platform-ws": "8.4.6",
    "@nestjs/serve-static": "2.2.2",
    "@nestjs/websockets": "8.4.6",
    "@types/compression": "1.7.2",
    "@types/express": "4.17.13",
    "@types/node": "17.0.41",
    "@types/websocket": "1.0.5",
    "@types/ws": "8.5.3",
    "class-transformer": "0.5.1",
    "class-validator": "0.13.2",
    "compression": "1.7.4",
    "dotenv": "16.0.1",
    "express": "4.18.1",
    "firebase-admin": "10.3.0",
    "firebase-functions": "3.21.2",
    "reflect-metadata": "0.1.13",
    "rxjs": "7.5.5",
    "tslib": "2.4.0"
  },
  "engines": {
    "node": "10",
    "npm": ">=6.14.4"
  }
}
