{
  "name": "upgraded-enigma-server",
  "private": true,
  "description": "Cloud Functions for Firebase",
  "main": "main.js",
  "scripts": {
    "deploy": "firebase deploy --only functions",
    "dist:clean": "rm -f ./main.js ./main.js.map ./*.txt",
    "dist:get": "npm run dist:clear; cp -r ../dist/apps/api/* ./",
    "logs": "firebase functions:log",
    "serve": "firebase serve --only functions",
    "shell": "firebase experimental:functions:shell",
    "start": "npm run shell",
    "start:emulators": "firebase emulators:start --only functions"
  },
  "dependencies": {
    "@nestjs/axios": "0.1.0",
    "@nestjs/common": "9.0.4",
    "@nestjs/core": "9.0.4",
    "@nestjs/jwt": "9.0.0",
    "@nestjs/microservices": "9.0.4",
    "@nestjs/passport": "9.0.0",
    "@nestjs/platform-express": "9.0.4",
    "@nestjs/platform-ws": "9.0.4",
    "@nestjs/serve-static": "3.0.0",
    "@nestjs/websockets": "9.0.4",
    "@sentry/node": "7.7.0",
    "@sentry/tracing": "7.7.0",
    "@types/compression": "1.7.2",
    "@types/express": "4.17.13",
    "@types/node": "18.0.6",
    "@types/websocket": "1.0.5",
    "@types/ws": "8.5.3",
    "body-parser": "1.20.0",
    "class-transformer": "0.5.1",
    "class-validator": "0.13.2",
    "compression": "1.7.4",
    "dotenv": "16.0.1",
    "express": "4.18.1",
    "firebase-admin": "11.0.0",
    "firebase-functions": "3.22.0",
    "reflect-metadata": "0.1.13",
    "rxjs": "7.5.6",
    "tslib": "2.4.0"
  },
  "engines": {
    "node": "10",
    "npm": ">=6.14.4"
  }
}
