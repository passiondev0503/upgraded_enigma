{
  "name": "upgraded-enigma-server",
  "private": true,
  "description": "Cloud Functions for Firebase",
  "main": "main.js",
  "scripts": {
    "deploy": "firebase deploy --only functions",
    "dist:clean": "rm -f ./main.js ./main.js.map ./*.txt",
    "dist:get": "npm run dist:clear; cp -r ../dist/apps/api/* ./",
    "logs": "firebase functions:log",
    "serve": "firebase serve --only functions",
    "shell": "firebase experimental:functions:shell",
    "start": "npm run shell",
    "start:emulators": "firebase emulators:start --only functions"
  },
  "dependencies": {
    "@nestjs/axios": "0.0.7",
    "@nestjs/common": "8.4.4",
    "@nestjs/core": "8.4.4",
    "@nestjs/jwt": "8.0.0",
    "@nestjs/microservices": "8.4.4",
    "@nestjs/passport": "8.2.1",
    "@nestjs/platform-express": "8.4.4",
    "@nestjs/platform-ws": "8.4.4",
    "@nestjs/websockets": "8.4.4",
    "class-transformer": "0.5.1",
    "class-validator": "0.13.2",
    "dotenv": "16.0.0",
    "express": "4.17.3",
    "firebase-admin": "10.0.2",
    "firebase-functions": "3.20.0",
    "keypair": "1.0.4",
    "pem": "1.14.6",
    "reflect-metadata": "0.1.13",
    "rxjs": "7.5.5"
  },
  "engines": {
    "node": "10",
    "npm": ">=6.14.4"
  }
}
